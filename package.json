{
	"name": "iapp",
	"displayName": "F5 Networks iApp",
	"description": "F5 Networks iApp Extension",
	"publisher": "bitwisecook",
	"version": "1.0.3",
	"engines": {
		"vscode": "^1.35.0"
	},
	"icon": "images/f5.png",
	"galleryBanner": {
		"color": "#DF0832",
		"theme": "dark"
	},
	"homepage": "https://github.com/bitwisecook/vscode-iApp/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/bitwisecook/vscode-iApp.git"
	},
	"categories": [
		"Other",
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:iapp-lang",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [],
		"languages": [
			{
				"id": "iapp-lang",
				"aliases": [
					"iapp",
					"iApp"
				],
				"extensions": [
					"implementation",
					"macro",
					".iapp",
					".tcl"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "apl-lang",
				"aliases": [
					"iapp-apl",
					"presentation"
				],
				"extensions": [
					"presentation",
					".apl"
				]
			}
		],
		"grammars": [
			{
				"language": "iapp-lang",
				"scopeName": "source.iapp",
				"path": "./out/syntaxes/iapp.tmLanguage"
			},
			{
				"language": "apl-lang",
				"scopeName": "source.apl",
				"path": "./out/syntaxes/apl.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "iapp-lang",
				"path": "./snippets/iapp.json"
			},
			{
				"language": "iapp-lang",
				"path": "./snippets/iapp-aglib.json"
			},
			{
				"language": "apl-lang",
				"path": "./snippets/apl.json"
			},
			{
				"language": "apl-lang",
				"path": "./snippets/apl-aglib.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./ && node out/build.js",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"converter": "0.0.5",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	}
}

{
    "name": "iapp",
    "displayName": "F5 Networks iApp",
    "description": "F5 Networks iApp Extension",
    "publisher": "bitwisecook",
    "version": "1.1.1",
    "keywords": [
        "f5",
        "f5networks",
        "iapp",
        "iapps"
    ],
    "engines": {
        "vscode": "^1.45.0"
    },
    "bugs": {
        "url": "https://github.com/bitwisecook/vscode-iApp/issues"
    },
    "license": "https://github.com/bitwisecook/vscode-iApp/blob/master/LICENSE",
    "icon": "images/f5.png",
    "galleryBanner": {
        "color": "#DCDCDC",
        "theme": "light"
    },
    "homepage": "https://github.com/bitwisecook/vscode-iApp/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/bitwisecook/vscode-iApp.git"
    },
    "categories": [
        "Formatters",
        "Other",
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:iapp-lang",
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [],
        "languages": [
            {
                "id": "iapp-lang",
                "aliases": [
                    "iapp",
                    "iApp"
                ],
                "extensions": [
                    "implementation",
                    "macro",
                    ".iapp",
                    ".macro",
                    ".tcl"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "apl-lang",
                "aliases": [
                    "iapp-apl",
                    "presentation"
                ],
                "extensions": [
                    "presentation",
                    ".apl"
                ]
            }
        ],
        "grammars": [
            {
                "language": "iapp-lang",
                "scopeName": "source.iapp",
                "path": "./out/syntaxes/iapp.json"
            },
            {
                "language": "apl-lang",
                "scopeName": "source.apl",
                "path": "./out/syntaxes/apl.json"
            }
        ],
        "snippets": [
            {
                "language": "iapp-lang",
                "path": "./snippets/iapp.json"
            },
            {
                "language": "iapp-lang",
                "path": "./snippets/iapp-aglib.json"
            },
            {
                "language": "apl-lang",
                "path": "./snippets/apl.json"
            },
            {
                "language": "apl-lang",
                "path": "./snippets/apl-aglib.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./ && npx js-yaml syntaxes/iapp.tmlanguage.yaml > out/syntaxes/iapp.json && npx js-yaml syntaxes/apl.tmlanguage.yaml > out/syntaxes/apl.json",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.2",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.13.12",
        "@types/vscode": "^1.46.0",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.9.5",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "js-yaml": "^3.14.0",
        "yamljs": "^0.3.0"
    }
}
